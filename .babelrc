{
    "env": {
        "development": {
            "presets": ["env", "stage-0", "react"]
        },
        "build-ui": {
            "presets": [
                [
                  "env",
                  {
                    "modules": false
                  }
                ],
                "stage-0", "react"
              ],
              "plugins": [
                "external-helpers",
                "transform-flow-strip-types"
              ]
        },
        "test": {
            "presets": ["env", "stage-0", "react"]
        }
    },
    "plugins": [
        "transform-flow-strip-types"
    ]
}